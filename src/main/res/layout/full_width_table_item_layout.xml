<?xml version="1.0" encoding="utf-8"?><com.andexert.library.RippleView    android:id="@+id/more"    xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:blurkit="http://schemas.android.com/tools"    android:layout_width="match_parent"    app:rv_rippleDuration="100"    android:layout_height="wrap_content">    <androidx.constraintlayout.widget.ConstraintLayout        android:id="@+id/clickable_part"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:background="@color/colorPrimary"        android:paddingTop="2dp"        android:paddingBottom="4dp">        <RelativeLayout            android:id="@+id/rl"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_marginStart="24dp"            android:layout_marginTop="8dp"            android:layout_marginBottom="8dp"            app:layout_constraintBottom_toBottomOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent">            <de.hdodenhof.circleimageview.CircleImageView                android:id="@+id/avtar"                android:layout_width="120dp"                android:layout_height="120dp"                android:layout_centerInParent="true"                app:civ_border_color="#222323"                app:civ_border_width="1dp" />            <RelativeLayout                android:id="@+id/is_live_relative"                android:layout_width="120dp"                android:layout_height="120dp"                android:visibility="gone">                <de.hdodenhof.circleimageview.CircleImageView                    android:layout_width="145dp"                    android:layout_height="145dp"                    android:layout_centerInParent="true"                    android:src="#80151515"                    app:civ_border_color="#3E3F40"                    app:civ_border_overlay="true"                    app:civ_border_width="1dp" />                <ImageView                    android:layout_width="50dp"                    android:layout_height="50dp"                    android:layout_centerInParent="true"                    android:src="@drawable/ic_live" />            </RelativeLayout>        </RelativeLayout>        <LinearLayout            android:id="@+id/main_ln"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_marginTop="22dp"            android:orientation="vertical"            android:paddingStart="32dp"            app:layout_constraintEnd_toStartOf="@+id/menu"            app:layout_constraintStart_toEndOf="@+id/rl"            app:layout_constraintTop_toTopOf="@+id/rl">            <LinearLayout                android:id="@+id/if_title_empty_linear"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:orientation="vertical"                android:visibility="gone">                <TextView                    android:id="@+id/no_title"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:fontFamily="@font/montr_semi"                    android:text="No Title"                    android:textColor="@color/themetextsecondarycolor"                    android:textSize="14sp"                    android:textStyle="italic" />                <ImageView                    android:layout_width="match_parent"                    android:layout_height="5dp"                    android:src="@drawable/doted_lines_drawable" />            </LinearLayout>            <TextView                android:id="@+id/textView32"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginEnd="8dp"                android:ellipsize="end"                android:fontFamily="@font/montr_extrabold"                android:lineSpacingExtra="-3dp"                android:maxLines="2"                android:letterSpacing="0.01"                android:textColor="@color/themetextcolor"                android:textSize="15sp"                app:layout_constraintEnd_toStartOf="@id/menu"                app:layout_constraintStart_toEndOf="@+id/avtar"                app:layout_constraintTop_toTopOf="@+id/avtar" />        </LinearLayout>        <com.google.android.material.button.MaterialButton            android:id="@+id/menu"            style="@style/Widget.MaterialComponents.Button.TextButton"            android:layout_width="21dp"            android:layout_height="35dp"            android:layout_marginTop="18dp"            android:layout_marginEnd="12dp"            android:paddingStart="0dp"            app:icon="@drawable/vertical_menu"            app:iconSize="20dp"            app:iconTint="@color/allicontintcolor"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintTop_toTopOf="@+id/rl" />        <LinearLayout            android:layout_width="0dp"            android:layout_height="wrap_content"            android:orientation="vertical"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toEndOf="@+id/rl"            app:layout_constraintTop_toBottomOf="@+id/main_ln">            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:orientation="vertical">                <TextView                    android:id="@+id/textView33"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginStart="32dp"                    android:layout_marginEnd="8dp"                    android:ellipsize="end"                    android:fontFamily="@font/montr_bold"                    android:maxLines="1"                    android:textColor="@color/themetextsecondarycolor"                    android:textSize="15sp" />                <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="2dp"                    android:orientation="horizontal">                    <TextView                        android:id="@+id/total_subscriber"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginStart="32dp"                        android:letterSpacing="0.01"                        android:fontFamily="@font/montr_bold"                        android:textColor="@color/themetextsecondarycolor"                        android:textSize="13sp" />                    <androidx.cardview.widget.CardView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginStart="8dp"                        app:cardBackgroundColor="@color/colorPrimary"                        app:cardElevation="0dp">                        <ImageView                            android:id="@+id/already_subscribed_indicator"                            android:layout_width="30dp"                            android:layout_height="22dp"                            android:fontFamily="@font/nunito_bold"                            android:paddingTop="2dp"                            android:paddingEnd="8dp"                            android:paddingBottom="4dp"                            android:src="@drawable/notification"                            android:textColor="@color/themetextsecondarycolor"                            android:textSize="11sp"                            android:visibility="gone" />                    </androidx.cardview.widget.CardView>                </LinearLayout>            </LinearLayout>        </LinearLayout>    </androidx.constraintlayout.widget.ConstraintLayout></com.andexert.library.RippleView>